{
  "name": "opencage/geocode",
  "description": "A PHP library for geocoding via the OpenCage Geocoder API",
  "license": "BSD-2-Clause",
  "authors": [
    {
      "name": "OpenCage GmbH",
      "email": "support@opencagedata.com",
      "homepage": "https://opencagedata.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "OpenCage\\Geocoder\\": "src"
    }
  },
  "require": {
    "php": ">=8.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.5 || ^11.0 || ^12.0",
    "squizlabs/php_codesniffer": "^3.7 || ^4.0",
    "phpstan/phpstan": "^2.0" 
  },
  "scripts": {
    "test": "phpunit --display-deprecations",
    "test:nocurl": "SKIP_CURL=1 phpunit --display-deprecations",
    "phpcs": "phpcs .",
    "phpstan": "phpstan analyse",
    "check": [
      "composer validate --strict",
      "@test",
      "@test:nocurl",
      "@phpcs",
      "@phpstan"
    ]
  },
  "archive": {
    "exclude": ["vendor", ".DS_Store"]
  },
  "keywords": [
    "geocode",
    "addresses",
    "geocoding",
    "opencagedata"
  ],
  "support": {
    "issues": "https://github.com/OpenCageData/php-opencage-geocode/issues",
    "source": "https://github.com/OpenCageData/php-opencage-geocode",
    "docs": "https://opencagedata.com/api"
  }
}
